{
  "dependencies": {
    "interactjs": "^1.10.27",
    "money": "^0.2.0",
    "tabulator-tables": "^6.3.1"
  },
  "name": "mousehunt-iap2gold",
  "version": "0.0.3",
  "type": "module",
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "esbuild": "^0.27.0",
    "node-fetch": "^3.3.2"
  },
  "scripts": {
    "build:itemMap": "node generateItemMap.js",
    "build:esbuild": "node esbuild.config.js",
    "copy:icons": "copyfiles -u 2 src/extension/icons/* build/extension",
    "copy:html": "copyfiles -u 2 src/extension/*.html build/extension",
    "copy:css": "copyfiles -u 1 src/styles/*.css build",
    "copy:libs": "copyfiles -u 1 src/libs/* build",
    "copy:data": "copyfiles -u 2 src/data/itemMap.json build/data",
    "build:copy": "npm run copy:icons && npm run copy:html && npm run copy:css && npm run copy:libs && npm run copy:data",
    "build": "npm run build:itemMap && npm run build:esbuild && npm run build:copy"
  },
  "license": "GPL-3.0"
}
